{
  "Template": {
    "TemplateName": "TransactionRWATemplate",
    "SubjectPart": "RWA Alert: {{rwa_event_main}}!!",
    "HtmlPart": "<!DOCTYPE html>\n<html>\n  <head>\n    <meta charset=\"UTF-8\" />\n    <title>Transaction Notification</title>\n    <style>\n      body {\n        font-family: Arial, sans-serif;\n        color: #333333;\n        margin: 0;\n        padding: 0;\n      }\n      .container {\n        max-width: 600px;\n        margin: 0 auto;\n        padding: 24px;\n      }\n      .header {\n        padding: 16px 0;\n        text-align: center;\n        background-color: #f5f5f5;\n      }\n      .content {\n        background-color: #ffffff;\n        padding: 24px;\n        border: 1px solid #dddddd;\n      }\n      .footer {\n        text-align: center;\n        margin-top: 24px;\n        color: #aaaaaa;\n        font-size: 12px;\n      }\n    </style>\n  </head>\n  <body>\n    <div class=\"container\">\n      <div class=\"header\">\n        <h2>{{rwa_event_main}}!!</h2>\n      </div>\n      <div class=\"content\">\n        <p>Hello <strong>{{ name }}</strong>,</p>\n        <p>\n          RWA <strong>{{ rwa_name }}</strong>\n          has been <strong>{{ rwa_event_sub }}</strong>.\n        </p>\n        <p>\n          <a href=\"{{ rwa_url }}\" style=\"display: inline-block; padding: 10px 20px; color: #ffffff; background-color: #007bff; text-decoration: none; border-radius: 4px;\">\n            View RWA\n          </a>\n        </p>\n        <p>\n          Thank you for using our service. Please log in to your account\n          for more information.\n        </p>\n      </div>\n      <div class=\"footer\">\n        <p>© 2025 Vault-X. All rights reserved.</p>\n      </div>\n    </div>\n  </body>\n</html>",
    "TextPart": "Hello {{ name }},\n\nRWA {{ rwa_name }} has been {{ rwa_event_sub }}.\nView RWA: {{ rwa_url }}\n\nThank you for using our service.\n© 2025 Vault-X. All rights reserved."
  }
}
